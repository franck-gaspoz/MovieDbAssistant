  "Props": {
    "listMoviePicNotAvailable": "./img/image-not-availble-300x175.jpg",
    "detailMoviePicNotAvailable": "../img/image-not-availble-300x175.jpg",
    "listMoviePicNotFound": "./img/image-404-630x630.jpg",
