{
  "Id": "dark",
  "Name": "Dark",
  "Version": "1.0.0",
  "VersionDate": "2024/10/03",
  "Author": "Franck Gaspoz",
  "Theme": {
    "Ui": {
      "Id": "blue-neon",
      "Ver": "1.0.0"
    },
    "Fonts": {
      "id": "blue-neon",
      "ver": "1.0.0"
    },
    "Icons": {
      "Id": "blue-neon",
      "Ver": "1.0.0"
    },
    "Buttons": {
      "Id": "blue-neon",
      "Ver": "1.0.0"
    }
  },
  
  "Templates": {
    "List": "page-list.tpl.html",
    "Details": "page-details.tpl.html"
  },

  "Pages": [
